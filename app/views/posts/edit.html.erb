<h1>edit</h1>


<%= form_with model: @post, local: true do |f| %>
    <div class="form-group">
      <%= f.label :title%>
      <%= f.text_field :title, class: 'form-control' %>
    </div>
    <div class="form-group">
      <%= f.label :content %>
      <%= f.text_area :content, class: 'form-control' %>
    </div>
    <div class="actions">
      <%= f.submit 'Update Post', class: 'btn btn-primary' %>
    </div>
    <% end %>